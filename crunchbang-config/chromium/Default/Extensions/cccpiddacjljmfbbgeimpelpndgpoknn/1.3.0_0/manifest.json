{
   "background": {
      "scripts": [ "lib/au.min.js", "lib/background.js" ]
   },
   "content_scripts": [ {
      "all_frames": true,
      "css": [ "/assets/css/fish.css" ],
      "matches": [ "http://www.superfish.com/ws/*", "https://www.superfish.com/ws/*" ],
      "run_at": "document_start"
   }, {
      "all_frames": false,
      "js": [ "lib/au.min.js", "lib/support.js" ],
      "matches": [ "http://*/*", "https://*/*" ],
      "run_at": "document_start"
   }, {
      "all_frames": true,
      "js": [ "lib/sscr.js", "lib/middlemouse.js" ],
      "matches": [ "http://*/*", "https://*/*", "ftp://*/*" ],
      "run_at": "document_start"
   }, {
      "all_frames": true,
      "js": [ "lib/support_frame.js" ],
      "matches": [ "http://*.yieldsquare.com/*", "https://*.yieldsquare.com/*" ],
      "run_at": "document_start"
   }, {
      "js": [ "lib/thanks.js" ],
      "matches": [ "http://www.smoothscrollapp.com/thanks/*" ],
      "run_at": "document_start"
   } ],
   "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'",
